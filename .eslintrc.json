{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "class-methods-use-this": ["error", { "exceptMethods": ["getData"] }],
    "no-console":"off",
    "no-multi-spaces":"off",
    "linebreak-style":"off",
    "key-spacing": ["error", { "mode": "minimum" }],
    "no-prototype-builtins":"off",
    "one-var": ["error", {"initialized": "never", "uninitialized": "always"}],
    "one-var-declaration-per-line":"off",
    "no-underscore-dangle": 0
  }
}
